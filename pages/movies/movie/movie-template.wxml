<import src="/pages/movies/stars/stars-template.wxml" />
<template name="movieTemplate">
    <view class="movie-container" catchtap="onMovieTap" data-movieId="{{movieId}}">
        <image src="{{coverageUrl}}" class="movie-img"></image>
        <text class="movie-title">{{title}}</text>
        <template is="starsTemplate" data="{{score:average,stars:stars}}"/>
    </view>
</template>